---
import Layout from "@/layouts/Layout.astro";
import Link from "@/components/Link.astro";
import data from "../../../../data/STRchive-database.json";
---

<Layout title="Data">
  <section>
    <h1>Data</h1>

    <p>
      {
        data.map((d) => (
          <>
            <Link to={`/data/${d.gene}`}>{d.gene}</Link>
            <br />
          </>
        ))
      }
    </p>
  </section>
</Layout>
